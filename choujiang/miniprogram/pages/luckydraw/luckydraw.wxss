.container{
  width: 100%;
  height: 100%;
  background: red;
}
/* 上部 */
.top{
  width: 100vw;
  height: auto;
  position: relative;
}
.top image{
  width:100vw;
  height:300rpx;
}
.top .msg{
  width: 100%;
  position: absolute;
  bottom: 18rpx;
  text-align: center;
  color: #fff;
  font-size: 30rpx;
}
/* 中间 */
.middle {
  width: 100vw;
  height: 650rpx;
  position: relative;
}
.middle image{
  width: 100vw;
  height: 650rpx;
}
.middle .start{
  width: 220rpx;
  height: 220rpx;
  position: absolute;
  left:0;
  top: 0;
  right: 0;
  bottom: 0;
  z-index: 999;
  margin: auto;
  border: none;
}
.middle .luck-gds{
  width: 608rpx;
  height: 608rpx;
  box-sizing: border-box;
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  margin: auto;
  padding:10rpx;
  align-content:space-between;
  z-index: 998;
}
.middle .luck-gds .item{
  width: 155rpx;
  height: 155rpx;
  box-sizing: border-box;
  border-radius: 10rpx;
  color: #6e3033;
  font-size: 28rpx;
  text-align:center;
  line-height: 155rpx;
  background-color: #fffffd;
  border: 8rpx solid #fff;
}
.middle .luck-gds .active{
  border: 8rpx solid #ffbc33;
}
.middle .luck-gds .item:nth-child(5){
  visibility: hidden;
}
.bottom{
  width: 100%;
  height: 300rpx;
  position: relative;
  z-index: 998;
}
.bottom image{
  width: 100%;
  height: 300rpx;
}
.zhongjian{
  width: 473rpx;
  height: 504rpx;
  position: fixed;
  top:0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto; 
  z-index: 1300;
}
.zhongjian image{
  width: 473rpx;
  height: 504rpx;
}
.zhongjian .zhong-text{
  width: 100%;
  text-align: center;
  font-size: 28rpx;
  color: #ff7976;
  position: absolute;
  top: 80rpx;
}
.zhongjian .zhong-btn{
  width: 300rpx;
  height: 60rpx;
  line-height: 60rpx;
  border-radius: 30rpx;
  background: #fee98c;
  text-align: center;
  font-size:32rpx;
  color: #cd4e01;
  position: absolute;
  bottom: 20rpx;
  left: 0;
  right: 0;
  margin: auto;
}
